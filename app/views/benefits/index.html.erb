<section class="section">
  <div class="container">
    <h1 class="title is-1">Benefits</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Category</th>
        </tr>
      </thead>
      <tbody id="benefit-list">
        <% @benefits.each do |benefit| %>
          <tr>
            <td><%= benefit.name %></td>
            <td><%= benefit.category %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</section>
